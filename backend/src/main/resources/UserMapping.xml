<hibernate-mapping>

    <class name="UserEntiye" table="User">

        <id name="id" type="int" column="U_ID">
            <generator class="native"/>
        </id>

        <property name="benutzerName" column="U_Benutzername" type="string"/>
        <property name="eMail" column="U_Email" type="string"/>
        <property name="vorname" column="U_Vorname" type="string"/>
        <property name="nachname" column="U_Nachname" type="string"/>
        <property name="geschlechtId" column="U_Geschlecht_ID" type="int"/>
        <property name="rolleId" column="U_Rolle_ID" type="int"/>


    </class>
</hibernate-mapping>
